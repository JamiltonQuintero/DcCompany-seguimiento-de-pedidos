@model TallerCuatro.Models.Entities.Transportadora

@{
    ViewData["Title"] = "Details";
}

@section Styles{

}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">Detalles</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Paquetes">Gestión Paquetes</a></li>
                        <li class="breadcrumb-item active"><a asp-action="Create" asp-controller="Transportadoras">Gestión de Transportadoras</a>Crear Transportadora</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-6">

                    <div>
                        <h4>Paqu ete</h4>
                        <hr />
                        <dl class="row">
                            <dt class="col-sm-2">

                            </dt>
                            <dd class="col-sm-10">

                            </dd>

                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.Nombre)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.Nombre)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.CiudadSede)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.CiudadSede)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.Direccion)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.Direccion)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.Telefono)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.Telefono)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.Rut)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.Rut)
                            </dd>

                        </dl>
                    </div>
                    <div>
                        <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.TransportadoraId">Editar</a> |
                        <a class="btn btn-secondary" asp-action="Index">Regresar</a>
                    </div>

                </div>
                <!-- /.col-md-6 -->
            </div>
            <!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->

@section Scripts{

    <script src="~/js/tabla.js"></script>

}







